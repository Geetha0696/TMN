{
  "name": "api",
  "version": "1.0.0",
  "description": "Timesheet Management",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "DEBUG=express:* nodemon app.js",
    "serve": "nodemon --inspect app.js",
    "migrate": "npx sequelize-cli db:migrate --config=config/database.json",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --config=config/database.json",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all --config=config/database.json",
    "seed": "npx sequelize-cli db:seed:all --config=config/database.json",
    "seed:undo": "npx sequelize-cli db:seed:undo --config=config/database.json",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all --config=config/database.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.2",
    "nodemon": "^2.0.22",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.1"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.0"
  }
}
